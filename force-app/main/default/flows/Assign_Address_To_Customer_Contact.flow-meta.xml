            <conditions>
                <leftValueReference>Retrieve_Contact_for_Address_Update.OtherStreet</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Contact_Other_Address_Update</targetReference>
            </connector>
            <label>Other Address Does Not Exist</label>
        </rules>
    </decisions>
