<apex:page controller="OpenCases">
    <apex:pageBlock title="Open Cases">
        <apex:pageBlockTable value="{!OpenCases}" var="cs">
            <apex:column headerValue="Case Number" value="{!cs.CaseNumber}"></apex:column>
            <apex:column headerValue="Status" value="{!cs.Status}"></apex:column>
            <apex:column headerValue="Subject" value="{!cs.Subject}"></apex:column>
            <apex:column headerValue="Description" value="{!cs.Description}"></apex:column>
        </apex:pageBlockTable>
    </apex:pageBlock>
</apex:page>